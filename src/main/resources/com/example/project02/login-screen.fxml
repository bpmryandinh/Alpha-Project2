<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.project02.LoginScreen">
   <children>
      <Label layoutX="45.0" layoutY="57.0" text="Login">
         <font>
            <Font name="Arial Bold" size="35.0" />
         </font>
      </Label>
      <TextField fx:id="emailField" layoutX="85.0" layoutY="152.0" prefHeight="25.0" prefWidth="215.0" promptText="example@email.com" />
      <Label layoutX="85.0" layoutY="135.0" text="Email" />
      <PasswordField fx:id="passwordField" layoutX="85.0" layoutY="223.0" prefHeight="25.0" prefWidth="215.0" promptText="********" />
      <Label layoutX="85.0" layoutY="206.0" text="Password" />
      <Button fx:id="loginButton" layoutX="109.0" layoutY="295.0" mnemonicParsing="false" onAction="#onLoginButtonPressed" text="Login" />
      <Button layoutX="222.0" layoutY="295.0" mnemonicParsing="false" onAction="#switchToStart" text="Cancel" />
   </children>
</AnchorPane>
